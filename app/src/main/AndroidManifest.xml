<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dahei.me.designpattern">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design">
        <activity
            android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:label="Strategy Pattern"
            android:name=".Behavioral.Strategy.StrategyActivity">
        </activity>
        <activity
            android:label="Factory Method Pattern"
            android:name=".CreationalPattern.FactoryMethod.FactoryMethodActivity">
        </activity>
        <activity android:name=".CreationalPattern.AbstractFactory.AbstractFactoryActivity"
            android:label=" AbstractFactory"/>
        <activity android:name=".CreationalPattern.Builder.BuilderActivity"
            android:label="Builder"/>
        <activity android:name=".CreationalPattern.Prototype.PrototypeActivity"
            android:label="Prototype"/>
        <activity android:name=".CreationalPattern.Singleton.SingletonActivity"
            android:label="Singleton"/>
        <activity android:name=".Structural.Proxy.ProxyActivity"
            android:label="Proxy"/>
        <activity android:name=".Structural.AdapterPattern.AdapterActivity"
            android:label="Adapter"/>
    </application>

</manifest>